@{
    ViewData["Title"] = "Details";
    ViewData["Menu"] = "Employee";
    Layout = "~/Views/Shared/_TinLayout.cshtml";
    EmployeeModel em = ViewBag.Employee;
}

<div class="modal fade" id="employeeDetail">
    <section>
        <h1>Details Employee</h1>
        <div>
            <p>Id: @em.Id</p>
            <p>Name: @em.Name</p>
            <p>Surname: @em.Surname</p>
            <p>Gender: @em.Gender</p>
            <p>Document: @em.Document</p>
            <p>Doc. Number: @em.DocNumber</p>
        </div>
    </section>
</div>
